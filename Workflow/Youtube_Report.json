    {
      "parameters": {
        "jsCode": "return items.map(item => {\n  const title = item.json.Title || \"Untitled\";\n  const views = item.json.ViewCount || 0;\n  const likes = item.json.LikeCount || 0;\n  const comments = item.json.CommentCount || 0;\n  const totalFeedback = item.json.Total || 0;\n\n  const chartConfig = {\n    type: \"bar\",\n    data: {\n      labels: [title],\n      datasets: [\n        {\n          label: \"Positive\",\n          backgroundColor: \"green\",\n          data: [item.json.Positive],\n        },\n        {\n          label: \"Negative\",\n          backgroundColor: \"red\",\n          data: [item.json.Negative],\n        },\n        {\n          label: \"Neutral\",\n          backgroundColor: \"gray\",\n          data: [item.json.Neutral],\n        }\n      ]\n    },\n    options: {\n      title: {\n        display: true,\n        text: `üé¨ ${title}\\nüëÅÔ∏è ${views} views | üëç ${likes} likes | üí¨ ${comments} comments | üí° ${totalFeedback} analyzed`\n      },\n      plugins: {\n        legend: { position: 'top' },\n        title: {\n          display: true,\n          text: `Sentiment for \"${title}\"`,\n        }\n      }\n    }\n  };\n\n  const chartUrl = `https://quickchart.io/chart?c=${encodeURIComponent(JSON.stringify(chartConfig))}`;\n\n  return {\n    json: {\n      ...item.json,\n      chartUrl,\n    }\n  };\n});\n"
      },
